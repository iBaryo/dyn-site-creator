{
  "name": "dyn-site-creator",
  "version": "1.0.0",
  "description": "easily create pre configured test sites",
  "main": "index.js",
  "scripts": {
    "build": "tsc index.ts --target es2016 --module commonjs --sourcemap --declaration",
    "prepublish": "npm run build",
    "example": "browserify example/mySdk.ts -p [ tsify --project ./tsconfig.json ] -d -o example/mySdk.js && http-server",
    "test": "npm run build && jasmine-ts \"src/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iBaryo/dyn-site-creator"
  },
  "keywords": [
    "test",
    "site",
    "wizard"
  ],
  "author": "Or Bar Yosef",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.11.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.1.3",
    "jasmine-ts": "^0.2.1"
  },
  "peerDependencies": {
    "express": "^4.16.3"
  }
}
